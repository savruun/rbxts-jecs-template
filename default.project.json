{
  "name": "JECS Template",
  "globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
  "tree": {
    "$className": "DataModel",
    "ServerScriptService": {
      "$className": "ServerScriptService",
      "server": {
        "$path": "out/server"
      }
    },
    "ServerStorage": {
      "$className": "ServerStorage",
      "archive": {
        "$path": "out/archive"
      }
    },
    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "rbxts_include": {
        "$path": "include",
        "node_modules": {
          "$className": "Folder",
          "@rbxts": {
            "$path": "node_modules/@rbxts"
          },
          "@flamework": {
            "$path": "node_modules/@flamework"
          },
          "@rbxts-js": {
            "$path": "node_modules/@rbxts-js"
          },
          "@kunosyn": {
            "$path": "node_modules/@kunosyn"
          }
        }
      },
      "shared": {
        "$path": "out/shared"
      },
      "client": {
        "$path": "out/client"
      },
      "types": {
        "$path": "out/types"
      }
    },
    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "runtime": {
          "$path": "out/runtime.client.luau"
        }
      }
    },
    "Workspace": {
      "$className": "Workspace",
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "HttpService": {
      "$className": "HttpService",
      "$properties": {
        "HttpEnabled": true
      }
    },
    "SoundService": {
      "$className": "SoundService",
      "$properties": {
        "RespectFilteringEnabled": true
      }
    },
    "TestService": {
      "$className": "TestService",
      "$properties": {
        "ExecuteWithStudioRun": true
      },
      "$path": "test"
    }
  }
}
